local function GAZO_fake_script() -- destroyall.LocalScript 
	local script = Instance.new('LocalScript', destroyall)

	script.Parent.MouseButton1Click:Connect(function()
		-- Function to destroy all descendants of a given instance
		local function destroyDescendants(instance)
			local descendants = instance:GetDescendants()
			for _, descendant in ipairs(descendants) do
				descendant:Destroy()
			end
		end
	
		-- Function to destroy the entire Workspace
		local function destroyWorkspace()
			destroyDescendants(game.Workspace)
		end
	
		-- Call destroyWorkspace function to destroy everything in Workspace
		destroyWorkspace()
	end)
end
