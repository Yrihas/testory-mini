local function ZIBT_fake_script() -- _2baseplate.LocalScript 
	local script = Instance.new('LocalScript', _2baseplate)

	script.Parent.MouseButton1Click:Connect(function()
		-- Configuration
		local burgerSize = 100000  -- Size of the burger in centimeters
	
		-- Create the main burger model
		local burger = Instance.new("Model")
		burger.Name = "GiantBurger"
		burger.Parent = game.Workspace
	
		-- Function to create a burger part
		local function createBurgerPart(name, size, position, color)
			local part = Instance.new("Part")
			part.Name = name
			part.Size = size
			part.Position = position
			part.BrickColor = BrickColor.new(color)
			part.Anchored = true
			part.Parent = burger
		end
	
		-- Create burger parts
		-- Bottom bun
		createBurgerPart("BottomBun", Vector3.new(burgerSize, burgerSize / 10, burgerSize), Vector3.new(0, 0, 0), "Sand Yellow")
	
		-- Burger patty
		createBurgerPart("Patty", Vector3.new(burgerSize * 0.9, burgerSize / 20, burgerSize * 0.9), Vector3.new(0, burgerSize / 20, 0), "Bright orange")
	
		-- Cheese slice
		createBurgerPart("CheeseSlice", Vector3.new(burgerSize * 0.9, burgerSize / 40, burgerSize * 0.9), Vector3.new(0, burgerSize / 10, 0), "Bright yellow")
	
		-- Lettuce
		createBurgerPart("Lettuce", Vector3.new(burgerSize * 0.95, burgerSize / 40, burgerSize * 0.95), Vector3.new(0, burgerSize / 5, 0), "Lime green")
	
		-- Tomato slice
		createBurgerPart("TomatoSlice", Vector3.new(burgerSize * 0.85, burgerSize / 40, burgerSize * 0.85), Vector3.new(0, burgerSize / 4, 0), "Bright red")
	
		-- Top bun
		createBurgerPart("TopBun", Vector3.new(burgerSize, burgerSize / 10, burgerSize), Vector3.new(0, burgerSize / 2.5, 0), "Sand Yellow")
	end)
end
