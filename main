local function AFOAY_fake_script() -- DiscoLIghts.LocalScript 
	local script = Instance.new('LocalScript', DiscoLIghts)

	script.Parent.MouseButton1Click:Connect(function()
		local Workspace = game:GetService("Workspace")
		local RunService = game:GetService("RunService")
	
		-- Configuration
		local COLORS = {Color3.new(1, 0, 0), Color3.new(0, 1, 0), Color3.new(0, 0, 1), Color3.new(1, 1, 0), Color3.new(0, 1, 1), Color3.new(1, 0, 1)}
		local FLASH_INTERVAL = 0.2  -- Time interval between each flash
		local LIGHT_RADIUS = 100  -- Radius around the center of the map where lights will be placed
	
		-- Function to create and position a light
		local function createLight(position)
			local light = Instance.new("PointLight")
			light.Brightness = 5
			light.Range = 20
			light.Color = COLORS[math.random(#COLORS)]
	
			local part = Instance.new("Part")
			part.Anchored = true
			part.CanCollide = false
			part.Size = Vector3.new(1, 1, 1)
			part.Position = position
			part.Transparency = 1
			part.Parent = Workspace
	
			light.Parent = part
	
			return light
		end
	
		-- Function to generate random positions around the center of the map
		local function getRandomPosition(center, radius)
			local angle = math.random() * 2 * math.pi
			local distance = math.random() * radius
			local x = center.X + math.cos(angle) * distance
			local y = center.Y + math.random(-radius / 2, radius / 2)
			local z = center.Z + math.sin(angle) * distance
			return Vector3.new(x, y, z)
		end
	
		-- Center of the map
		local mapCenter = Workspace.Baseplate.Position
	
		-- Create initial lights
		local lights = {}
		for i = 1, 10 do
			table.insert(lights, createLight(getRandomPosition(mapCenter, LIGHT_RADIUS)))
		end
	
		-- Function to update light colors
		local function updateLights()
			for _, light in ipairs(lights) do
				light.Color = COLORS[math.random(#COLORS)]
			end
		end
	
		-- Flash lights at regular intervals
		RunService.Stepped:Connect(function()
			updateLights()
			wait(FLASH_INTERVAL)
		end)
	end)
end
