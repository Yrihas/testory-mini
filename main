local function FQLHIU_fake_script() -- Disco.LocalScript 
	local script = Instance.new('LocalScript', Disco)

	script.Parent.MouseButton1Click:Connect(function()
		local Players = game:GetService("Players")
		local Lighting = game:GetService("Lighting")
		local RunService = game:GetService("RunService")
	
		-- Function to generate random color
		local function getRandomColor()
			return Color3.fromRGB(math.random(0, 255), math.random(0, 255), math.random(0, 255))
		end
	
		-- Function to apply disco effect to a part
		local function applyDiscoEffect(part)
			local colorTweenInfo = TweenInfo.new(
				2, -- Duration
				Enum.EasingStyle.Linear, -- Easing style
				Enum.EasingDirection.InOut, -- Easing direction
				-1, -- Number of repetitions (-1 for infinite)
				true -- Reverses after completing
			)
	
			-- Tween the color of the part
			local tween = game:GetService("TweenService"):Create(part, colorTweenInfo, {Color = getRandomColor()})
			tween:Play()
		end
	
		-- Function to apply disco effect to players' characters
		local function applyDiscoToPlayers()
			for _, player in ipairs(Players:GetPlayers()) do
				if player.Character then
					local character = player.Character
					for _, part in ipairs(character:GetDescendants()) do
						if part:IsA("BasePart") then
							applyDiscoEffect(part)
						end
					end
				end
			end
		end
	
		-- Function to continuously apply disco effect
		local function startDiscoEffect()
			-- Apply to skybox
			Lighting.Ambient = getRandomColor()
			Lighting.OutdoorAmbient = getRandomColor()
	
			-- Apply to workspace
			for _, part in ipairs(workspace:GetDescendants()) do
				if part:IsA("BasePart") then
					applyDiscoEffect(part)
				end
			end
	
			-- Apply to players' characters
			applyDiscoToPlayers()
		end
	
		-- Start the disco effect
		startDiscoEffect()
	
		-- Continuously update the disco effect
		RunService.Stepped:Connect(function()
			startDiscoEffect()
		end)
	end)
end
