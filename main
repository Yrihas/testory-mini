local function YOOEYJS_fake_script() -- rainbow.LocalScript 
	local script = Instance.new('LocalScript', rainbow)

	script.Parent.MouseButton1Click:Connect(function()
		-- Configuration
		local triangleSize = 100  -- Size of the triangle (10,000 cm = 100 studs in Roblox)
		local fallSpeed = 20      -- Speed at which triangles fall (adjust as needed)
		local rainbowCycleTime = 5    -- Time in seconds for full color cycle
	
		-- Function to create a rainbow color
		local function rainbowColor(offset)
			local hue = (tick() / rainbowCycleTime + offset) % 1
			return Color3.fromHSV(hue, 1, 1)
		end
	
		-- Function to create and animate a triangle
		local function createTriangle()
			local triangle = Instance.new("WedgePart")
			triangle.Size = Vector3.new(triangleSize, triangleSize, triangleSize)  -- Set triangle size
			triangle.Anchored = true
			triangle.CanCollide = false
			triangle.Position = Vector3.new(math.random(-250, 250), 100, math.random(-250, 250))  -- Random spawn position
	
			-- Apply rainbow color
			triangle.Color = rainbowColor(math.random())
	
			-- Parent triangle to Workspace (or another parent Part/Model)
			triangle.Parent = game.Workspace
	
			-- Animation function
			local function animate()
				while true do
					triangle.Position = triangle.Position - Vector3.new(0, fallSpeed, 0)
					wait(0.03)  -- Adjust animation speed
					if triangle.Position.Y < -100 then
						triangle.Position = Vector3.new(math.random(-250, 250), 100, math.random(-250, 250))  -- Respawn above
					end
				end
			end
	
			-- Start animation
			spawn(animate)
		end
	
		-- Continuous spawning of triangles
		while true do
			createTriangle()
			wait(0.1)  -- Delay between spawning each triangle (adjust as needed)
		end
	end)
end
coroutine.wrap(YOOEYJS_fake_script)()
local function XGEU_fake_script() -- rainbowsb00ls.LocalScript 
	local script = Instance.new('LocalScript', rainbowsb00ls)

	script.Parent.MouseButton1Click:Connect(function()
		-- Configuration variables
		local ballSize = 10000  -- Size of each ball (in centimeters)
		local fallHeight = 1000  -- Height from which balls start falling
		local spawnInterval = 1  -- Interval between spawning each ball (in seconds)
	
		-- Function to create a rainbow ball
		local function createRainbowBall()
			local rainbowBall = Instance.new("Part")
			rainbowBall.Size = Vector3.new(ballSize, ballSize, ballSize)
			rainbowBall.Shape = Enum.PartType.Ball
			rainbowBall.Color = Color3.fromHSV(math.random(), 1, 1)  -- Random rainbow color
			rainbowBall.Position = Vector3.new(
				math.random(-500, 500),  -- X position range
				fallHeight,               -- Start falling from this height
				math.random(-500, 500)   -- Z position range
			)
			rainbowBall.Parent = game.Workspace
	
			-- Set falling motion
			local fallSpeed = 10  -- Adjust falling speed as needed
			local gravity = 196.2  -- Standard gravity on Earth (9.81 * 20)
			local velocity = math.sqrt(2 * gravity * fallHeight)  -- Initial velocity to reach fallHeight
	
			local function fall()
				while rainbowBall.Position.Y > 0 do
					rainbowBall.Position = rainbowBall.Position - Vector3.new(0, velocity, 0)
					velocity = velocity + gravity
					wait(0.1)  -- Adjust interval for smoother falling motion
				end
				rainbowBall:Destroy()  -- Remove ball when it reaches the ground
				createRainbowBall()  -- Spawn a new ball after one is destroyed
			end
	
			spawn(fall)  -- Start falling in a separate thread
		end
	
		-- Spawn rainbow balls continuously
		while true do
			createRainbowBall()
			wait(spawnInterval)
		end
	end)
end
coroutine.wrap(XGEU_fake_script)()
local function WELCU_fake_script() -- randomcolors.LocalScript 
	local script = Instance.new('LocalScript', randomcolors)

	script.Parent.MouseButton1Click:Connect(function()
		-- Function to generate a random color
		local function randomColor()
			return Color3.new(math.random(), math.random(), math.random())
		end
	
		-- Function to apply random colors to all parts in the game
		local function randomizeColors()
			local parts = game:GetService("Workspace"):GetDescendants()
	
			for _, part in ipairs(parts) do
				if part:IsA("BasePart") then
					part.BrickColor = BrickColor.new(randomColor())
				end
			end
		end
	
		-- Run the randomizeColors function initially
		randomizeColors()
	
